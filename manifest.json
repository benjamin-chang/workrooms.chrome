{
  "name": "Workrooms",
  "version": "0.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Workrooms"
  },
  "background": {
    "scripts": ["build/background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "storage",
    "background",
    "identity",
    "pushMessaging",
    "tabCapture",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}

