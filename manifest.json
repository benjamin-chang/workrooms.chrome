{
  "name": "Workrooms",
  "version": "0.0.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Workrooms"
  },
  "background": {
    "page": "/build/background.html"
  },
  "web_accessible_resources": [
    "build/background.js",
    "build/conference.js",
    "build/gravatars.css",
    "build/gravatars.js"
  ],
  "permissions": [
    "storage",
    "background",
    "identity",
    "pushMessaging",
    "tabCapture",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}

